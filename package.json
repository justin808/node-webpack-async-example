{
  "name": "node-webpack-async-example",
  "version": "1.0.0",
  "description": "Demonstrate es7 with webpack for node example",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:debug": "npm run test -- --debug-brk",
    "build": "webpack",
    "watch": "webpack -w",
    "lint": "npm run eslint && npm run jscs",
    "eslint": "eslint --ext .js,.jsx .",
    "jscs": "jscs --verbose ."
  },
  "author": "justin.gordon@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.29",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "1.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.10.0",
    "jscs": "^2.6.0",
    "sleep": "^3.0.0",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.3",
    "sleep": "^3.0.0"
  }
}
